package cn.gov.cnis.cssn.wssort;

import java.io.Serializable;
import java.util.List;



public class TTopicCategory implements Serializable{

	private static final long serialVersionUID = 1L;
	
	private Long id;
	private String cgy_uid;
	private String cgy_name;
	private String cgy_number;
	private String cgy_parentUid;
	private String cgy_memo;
	private String cgy_groupUid;
	private String cgy_recommend;
	private List<TTopicCatesult> tTopicCatesult;
	
	public TTopicCategory() {

	}

	public TTopicCategory(Long id, String cgyUid, String cgyName, String cgyNumber,
			String cgyParentUid, String cgyMemo, String cgyGroupUid, String cgyRecommend,
			List<TTopicCatesult> tTopicCatesult) {
		this.id = id;
		cgy_uid = cgyUid;
		cgy_name = cgyName;
		cgy_number = cgyNumber;
		cgy_parentUid = cgyParentUid;
		cgy_memo = cgyMemo;
		cgy_groupUid = cgyGroupUid;
		cgy_recommend = cgyRecommend;
		this.tTopicCatesult = tTopicCatesult;
	}

	public long getId() {
	
		return id == null ? 0 : Long.valueOf(id);
	}
	
	public void setId(Long id) {
	
		this.id = id;
	}
	
	public String getCgy_uid() {
	
		return cgy_uid;
	}
	
	public void setCgy_uid(String cgyUid) {
	
		cgy_uid = cgyUid;
	}
	
	public String getCgy_name() {
	
		return cgy_name;
	}
	
	public void setCgy_name(String cgyName) {
	
		cgy_name = cgyName;
	}
	
	public String getCgy_number() {
	
		return cgy_number;
	}
	
	public void setCgy_number(String cgyNumber) {
	
		cgy_number = cgyNumber;
	}
	
	public String getCgy_parentUid() {
	
		return cgy_parentUid;
	}
	
	public void setCgy_parentUid(String cgyParentUid) {
	
		cgy_parentUid = cgyParentUid;
	}
	
	public String getCgy_memo() {
	
		return cgy_memo;
	}
	
	public void setCgy_memo(String cgyMemo) {
	
		cgy_memo = cgyMemo;
	}
	
	public String getCgy_groupUid() {
	
		return cgy_groupUid;
	}
	
	public void setCgy_groupUid(String cgyGroupUid) {
	
		cgy_groupUid = cgyGroupUid;
	}
	
	public String getCgy_recommend() {
	
		return cgy_recommend;
	}
	
	public void setCgy_recommend(String cgyRecommend) {
	
		cgy_recommend = cgyRecommend;
	}

	
	public List<TTopicCatesult> gettTopicCatesult() {
	
		return tTopicCatesult;
	}

	
	public void settTopicCatesult(List<TTopicCatesult> tTopicCatesult) {
	
		this.tTopicCatesult = tTopicCatesult;
	}
}
